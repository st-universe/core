{% from 'html/macros.twig' import userAvatar %}

<div class="box">
    <div class="box_title">Profilbesucher</div>
    <div class="box_body">
        {% if RECENT_PROFILE_VISITORS %}
        <div class="recentProfileVisitors">
            {% for data in RECENT_PROFILE_VISITORS %}
            {{ userAvatar(data.getUser) }}
            {% endfor %}
        </div>
        {% else %}
        <div style="width: 100%; text-align: center;">
            Keine Besucher
        </div>
        {% endif %}
    </div>
</div>
<br />
