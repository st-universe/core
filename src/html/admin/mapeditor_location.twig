{% set map = mapItem.getMap %}
{% set fieldType = map.getFieldType %}
{% set passable = fieldType.getPassable ? 1 : 0 %}
<td class="starmap" id="{{ map.getCx }}_{{ map.getCy }}" data-passable="{{ passable }}"
    data-region="{{ map.getRegionId }}" data-admin-region="{{ map.getAdminRegionId }}"
    data-system-type-id="{{ map.getSystemTypeId }}" data-influence-area="{{map.getInfluenceAreaId}}"
    data-effects="{{ fieldType.getEffectsAsString }}" data-border="{{ map.getBorderColor}}">
    <div class="divbody lssobj"
        style="cursor: pointer ! important; padding: 0px !important; width: 30px; height: 30px; user-select: none; -webkit-user-select: none; -moz-user-select: none; -ms-user-select: none;"
        onclick="handleFieldClick(this,{{ map.getId }});" onmousedown="handleMouseDown(this, {{ map.getId }}, event);"
        onmouseenter="handleMouseEnter(this, {{ map.getId }});" onmouseover="toggleMapfieldType(this)"
        onmouseout="toggleMapfieldType(this)">
        <img src="/assets/map/{{ mapItem.getMapGraphicPath }}" draggable="false" />
    </div>
</td>