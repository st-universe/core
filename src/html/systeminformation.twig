{% if EXECUTEJSBEFORERENDER %}
<script>
    {% for item in EXECUTEJSBEFORERENDER %}
        {{ item|raw }}
    {% endfor %}
</script>
{% endif %}
<div id="result">
    <table class="use">
        <tr>
            <th>Meldung</th>
        </tr>
        <tr>
            <td>
                {% for info in INFORMATION %}
                <div>
                    {{ info|bbcode }}
                </div>
                {% endfor %}
                {% if TARGET_LINK %}
                <a href="{{ TARGET_LINK.getUrl }}" class="linkbutton">
                    {{ TARGET_LINK.getTitle }}
                </a>
                {% endif %}
            </td>
        </tr>
    </table>
</div>
<br />
