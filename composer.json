{
  "name": "st-universe/core",
  "description": "ST-Universe Browsergame Source Code",
  "license": "MIT",
  "autoload": {
    "classmap": [
      "src/Lib",
      "src/admin",
      "tests/"
    ],
    "psr-4": {
      "Stu\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Stu\\": [
        "tests/"
      ]
    }
  },
  "require": {
    "php": "^7.4 || 8.0",
    "ext-dom": "*",
    "ext-gd": "*",
    "ext-gettext": "*",
    "ext-json": "*",
    "ext-mysqli": "*",
    "ext-pcntl": "*",
    "ext-redis": "*",
    "ext-sockets": "*",
    "ext-sysvsem": "*",
    "ext-xsl": "*",
    "amenadiel/jpgraph": "^4",
    "cache/array-adapter": "^1.0",
    "cache/psr-6-doctrine-bridge": "^3.0",
    "cache/redis-adapter": "^1.0",
    "devster/ubench": "^2",
    "doctrine/annotations": "^1.13",
    "doctrine/orm": "^2.6",
    "filp/whoops": "^2.5",
    "graphp/graphviz": "^0.2.1",
    "hackzilla/password-generator": "^1.4",
    "hassankhan/config": "^2.0",
    "jbbcode/jbbcode": "^1.3",
    "json-mapper/json-mapper": "^2.14",
    "laminas/laminas-mail": "^2",
    "mnsami/composer-custom-directory-installer": "^2",
    "monolog/monolog": "^2.0",
    "mpscholten/request-parser": "^1.5",
    "php-di/php-di": "^6",
    "scn/phptal": "^3",
    "seld/signal-handler": "^1.2",
    "slim/http": "^1.2",
    "slim/psr7": "^0.6",
    "slim/slim": "^4.2",
    "st-universe/starsystem-generator": "dev-master",
    "usox/ipintel": "^1.0",
    "zendframework/zend-mail": "^2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.13",
    "mockery/mockery": "^1.1",
    "phpstan/phpstan": "^1.9",
    "phpstan/phpstan-mockery": "^1.1",
    "phpunit/phpunit": "^9",
    "rector/rector": "^0.15.10",
    "st-universe/assets": "dev-master"
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "sort-packages": true,
    "allow-plugins": {
      "mnsami/composer-custom-directory-installer": false
    }
  },
  "scripts": {
    "tests": "@php vendor/bin/phpunit tests",
    "stan": "phpstan",
    "rector:dry": "rector process -n",
    "coverage": "phpunit --warm-coverage-cache tests && XDEBUG_MODE=coverage ./vendor/bin/phpunit --coverage-html build/coverage tests",
    "cs:check": "php-cs-fixer fix --dry-run --diff",
    "cs:fix": "php-cs-fixer fix"
  },
  "extra": {
    "installer-paths": {
      "./src/Public/assets": [
        "st-universe/assets"
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:st-universe/assets.git"
    }
  ]
}
